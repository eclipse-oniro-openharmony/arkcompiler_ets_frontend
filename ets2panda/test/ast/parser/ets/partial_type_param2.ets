export interface I {
    i: number;
    j: string;
}

function foo<T extends I>(i: Partial<T>): number {
  let v: number = 0;
  if (typeof i === 'object') {
    if (i.j !== undefined) {
      v += 1;
    }
    if (i.i !== undefined && i.i !== 0) {
      v += 2;
    }
  }
  return v;
}

function main() {
  // Test case: only 'i' is provided, 'j' is omitted
  foo<I>({ i: 5 });
